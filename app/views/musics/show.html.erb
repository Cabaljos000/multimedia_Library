<%= render @music %>

<div class="flex flex-wrap items-center gap mbs-6" style="--row-gap: .5rem">
  <%= link_to "Edit this music", edit_music_path(@music), class: "btn" %>
  <%= link_to "Back to musics", musics_path, class: "btn" %>
  <%= button_to "Destroy this music", @music, method: :delete, class: "btn" %>
</div>
